--tablo olusturma---
CREATE TABLE student
(
id CHAR(4),
name VARCHAR(20),
age NUMBER
);
--VERI GIRISI--
INSERT INTO student VALUES('1001','MEHMET ALA',25);
INSERT INTO student VALUES('1002','AYSE ',12);

--tablodan veri sorgulama----
SELECT * FROM student;

--tablo silme--
DROP TABLE student;

-- PARCALI VERI GIRISI--
INSERT INTO student(id,name) VALUES('1003','FATMA');


--veri tabaninizda urunler adinda bir tablo olusturun--
--urun_id=>number
--urun_adi => VARCHAR(50)
--fiyat=> number
CREATE TABLE urunler
(
urun_id NUMBER,
urun_adi VARCHAR(50),
fiyat number
);
INSERT INTO urunler VALUES(100,'cips',5);
INSERT INTO urunler VALUES(101,'kola',6);
select * from urunler;